<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>loginMB</managed-bean-name>
		<managed-bean-class>com.counselling.bean.LoginMB</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>StudentLogin</display-name>
		<from-view-id>/StudentLogin.jsp</from-view-id>
		<navigation-case>
			<to-view-id>/StudentLogin.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
